<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Export">
            <items>
              <MenuItem mnemonicParsing="false" text="Catalogue" />
                  <MenuItem mnemonicParsing="false" text="Events" />
                  <MenuItem mnemonicParsing="false" text="Votes" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Help">
              <items>
                <MenuItem mnemonicParsing="false" text="Help" />
                  <MenuItem mnemonicParsing="false" text="About" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="Catalogue">
               <content>
                  <fx:include source="catalogue_tab.fxml" />
               </content></Tab>
            <Tab text="Students">
               <content>
                  <fx:include source="student_tab.fxml" />
               </content></Tab>
            <Tab text="Events">
               <content>
                  <fx:include source="event_tab.fxml" />
               </content></Tab>
            <Tab text="Vote">
               <content>
                  <fx:include source="vote_tab.fxml" />
               </content></Tab>
         </tabs>
      </TabPane>
   </children>
</VBox>
